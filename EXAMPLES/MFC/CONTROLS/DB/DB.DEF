LIBRARY      DB
DESCRIPTION  'DB OLE Control module (Win32)'

CODE         PRELOAD MOVEABLE DISCARDABLE
DATA         PRELOAD MOVEABLE SINGLE

HEAPSIZE     1024   ; initial heap size

EXPORTS
	DllCanUnloadNow 	@1
	DllGetClassObject	@2
	DllRegisterServer	@3
	DllUnregisterServer @4
