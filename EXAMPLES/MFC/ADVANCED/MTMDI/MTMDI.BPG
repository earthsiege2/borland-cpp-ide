#------------------------------------------------------------------------------
VERSION = BWS.01
#------------------------------------------------------------------------------
!ifndef ROOT
ROOT = $(MAKEDIR)\..
!endif
#------------------------------------------------------------------------------
MAKE = $(ROOT)\bin\make.exe
BPRTOMAK = $(ROOT)\bin\bprtomak.exe
DCC = $(ROOT)\bin\dcc32.exe
BRCC = $(ROOT)\bin\brcc32.exe
#------------------------------------------------------------------------------
PROJECTS = mtmdi 
#------------------------------------------------------------------------------
default: $(PROJECTS)
#------------------------------------------------------------------------------

mtmdi : mtmdi.bpr
   $(BPRTOMAK) mtmdi.bpr
   $(MAKE) -$(MAKEFLAGS) -fmtmdi.mak

