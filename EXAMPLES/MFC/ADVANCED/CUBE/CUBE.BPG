!ifndef ROOT
ROOT = $(MAKEDIR)\..
!endif
#------------------------------------------------------------------------------
MAKE = $(ROOT)\bin\make.exe
BPRTOMAK = $(ROOT)\bin\bprtomak.exe
DCC = $(ROOT)\bin\dcc32.exe
BRCC = $(ROOT)\bin\brcc32.exe
#------------------------------------------------------------------------------
PROJECTS = cube 
#------------------------------------------------------------------------------
default: $(PROJECTS)
#------------------------------------------------------------------------------

cube : cube.bpr
   $(BPRTOMAK) cube.bpr
   $(MAKE) -$(MAKEFLAGS) -fcube.mak

