bool [[TApplication]]::CanClose ()
{
    bool result = TApplication::CanClose();

##--BEGIN-- @QUERY_APPL_COMMENT == VALUE_VERBOSE
    //
    // Close the help engine if we used it.
    //
##--END-- @QUERY_APPL_COMMENT == VALUE_VERBOSE
