/******* Compatibility Macros ********/
MACRO WordLeft
  CursorLeftSkipNonWord;
  CursorLeftSkipWordStream;
  FixScreenPos;
END;

MACRO EpsilonMarkKillLine
  SetBlockBeg;
  IF InqCursorPastEOL THEN
    CursorCharRight
  ELSE
    RightOfLine
  ENDIF;
  SetBlockEnd;
  FixScreenPos;
END;

