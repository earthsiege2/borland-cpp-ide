MACRO copy_region
    HideBlock;
    SwapPrevPos;
    SetBlockBeg;
    SwapPrevPos;
    SetBlockEnd;
    HighlightBlock;
    IF InqValidBlock THEN
      BEGIN
        ClipCopy;
        ClipReplaceNextCut;
      END
    ELSE
      BEGIN
        HideBlock;
        SetBlockBeg;
        SwapPrevPos;
        SetBlockEnd;
        SwapPrevPos;
        HighlightBlock;
        IF InqValidBlock THEN
          BEGIN
            ClipCopy;
            ClipReplaceNextCut;
          END;
        ENDIF
      END
    ENDIF
END;

