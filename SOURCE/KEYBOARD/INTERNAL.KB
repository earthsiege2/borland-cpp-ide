MACRO MacLowerCaseWord
  IF NOT InqCursorPastEOF THEN
    BEGIN
      UndoGroupWithNone;
      MacMarkWord;LowerCaseBlock;HideBlock;MoveToTempPos;
    END
  ENDIF
END;

MACRO MacUpperCaseWord
  IF NOT InqCursorPastEOF THEN
    BEGIN
      UndoGroupWithNone;
      MacMarkWord;UpperCaseBlock;HideBlock;MoveToTempPos;
