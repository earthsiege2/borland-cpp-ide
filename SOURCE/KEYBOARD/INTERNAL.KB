MACRO MacISearch
    SetNonInclusiveBlock;HideBlock;IncrementalSearch;
END;

MACRO MacToggleCase
  UndoGroupWithNone;
  IF InqValidBlock THEN
    IF InqCursorInBlock THEN
      ToggleCase;
    ELSE
      BEGIN
        /* preserve original block marks and cursor */
        SetPrevPos;
        MoveToBlockBeg;
        SetTempPos;
        MoveToBlockEnd;
        SetTempEndPos;

        /* do case toggle */
        SetNonInclusiveBlock;
        MoveToPrevPos;
        SetBlockBeg;
        CursorCharRight;
        SetBlockEnd;
        ToggleCase;

        /* restore original block marks and cursor */
        MoveToTempPos;
        SetBlockBeg;
        MoveToTempEnd;
        SetBlockEnd;
        MoveToPrevPos;
      END
    ENDIF
  ELSE
    BEGIN
      SetNonInclusiveBlock;
      SetBlockBeg;
      CursorCharRight;
      SetBlockEnd;
      HighlightBlock;
      ToggleCase;
      HideBlock;
    END
  ENDIF;
END;

MACRO MacDeleteToEOL
  IF NOT InqCursorPastEOL THEN
    DeleteToEOL;
  ENDIF;
END;

MACRO MacBackspaceDelete
  IF InqCursorPastEOF THEN
    IF InqCursorAtBOL THEN
      BEGIN
        CursorUp;
        IF NOT InqCursorPastEOF THEN
          RightOfLine;
        ENDIF
      END
    ELSE
      CursorLeft;
    ENDIF
  ELSE
    BackspaceDelete
  ENDIF;
END;

